{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:player",
      "materials": {
        "default": "entity_alphatest",
        "cape": "entity_alphatest",
        "animated": "player_animated",
        "tractor_tool": "player_tractor_tool"
      },
      "textures": {
        "default": "textures/entity/steve",
        "cape": "textures/entity/cape_invisible",
        "tractor_tool": "textures/entity/tractor/tractor",
        "tractor_tool_headlight": "textures/entity/tractor/headlight_off"
      },
      "geometry": {
        "default": "geometry.humanoid.custom",
        "cape": "geometry.cape",
        "harvester": "geometry.player.harvester",
        "mower": "geometry.player.mower",
        "plow": "geometry.player.plow",
        "sprinkler": "geometry.player.sprinkler",
        "tractor_body_with_roof": "geometry.player.tractor_body_with_roof",
        "tractor_body_with_wheel": "geometry.player.tractor_body_with_wheel",
        "tractor_body_with_wheels": "geometry.player.tractor_body_with_wheels",
        "tractor_body": "geometry.player.tractor_body",
        "tractor_front_wheel": "geometry.player.tractor_front_wheel",
        "tractor_front_wheels": "geometry.player.tractor_front_wheels",
        "tractor_rear_wheel": "geometry.player.tractor_rear_wheel",
        "tractor_rear_wheels": "geometry.player.tractor_rear_wheels",
        "tractor_roof": "geometry.player.tractor_roof",
        "tractor_steering_wheel": "geometry.player.tractor_steering_wheel"
      },
      "scripts": {
        "scale": "0.9375",
        "initialize": [
          "variable.is_holding_right = 0.0;",
          "variable.is_blinking = 0.0;",
          "variable.last_blink_time = 0.0;",
          "variable.hand_bob = 0.0;"
        ],
        "pre_animation": [
          "variable.helmet_layer_visible = 1.0;",
          "variable.leg_layer_visible = 1.0;",
          "variable.boot_layer_visible = 1.0;",
          "variable.chest_layer_visible = 1.0;",
          "variable.attack_body_rot_y = Math.sin(360*Math.sqrt(variable.attack_time)) * 5.0;",
          "variable.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.first_person_rotation_factor = math.sin((1 - variable.attack_time) * 180.0);",
          "variable.hand_bob = query.life_time < 0.01 ? 0.0 : variable.hand_bob + ((query.is_on_ground && query.is_alive ? math.clamp(math.sqrt(math.pow(query.position_delta(0), 2.0) + math.pow(query.position_delta(2), 2.0)), 0.0, 0.1) : 0.0) - variable.hand_bob) * 0.02;",
          "variable.map_angle = math.clamp(1 - variable.player_x_rotation / 45.1, 0.0, 1.0);",
          "variable.item_use_normalized = query.main_hand_item_use_duration / query.main_hand_item_max_duration;",
          "variable.hand_tractor_tool = (query.get_equipped_item_name('main_hand') == 'harvester' || query.get_equipped_item_name('main_hand') == 'mower' || query.get_equipped_item_name('main_hand') == 'plow' || query.get_equipped_item_name('main_hand') == 'sprinkler' || query.get_equipped_item_name('main_hand') == 'tractor_body_with_roof' || query.get_equipped_item_name('main_hand') == 'tractor_body_with_wheel' || query.get_equipped_item_name('main_hand') == 'tractor_body_with_wheels' || query.get_equipped_item_name('main_hand') == 'tractor_body' || query.get_equipped_item_name('main_hand') == 'tractor_front_wheel' || query.get_equipped_item_name('main_hand') == 'tractor_front_wheels_on_axis' || query.get_equipped_item_name('main_hand') == 'tractor_rear_wheel' || query.get_equipped_item_name('main_hand') == 'tractor_rear_wheels_on_axis' || query.get_equipped_item_name('main_hand') == 'tractor_roof' || query.get_equipped_item_name('main_hand') == 'tractor_steering_wheel');",
          "variable.hand_harvester = query.get_equipped_item_name('main_hand') == 'harvester';",
          "variable.hand_mower = query.get_equipped_item_name('main_hand') == 'mower';",
          "variable.hand_plow = query.get_equipped_item_name('main_hand') == 'plow';",
          "variable.hand_sprinkler = query.get_equipped_item_name('main_hand') == 'sprinkler';",
          "variable.hand_tractor_body_with_roof = query.get_equipped_item_name('main_hand') == 'tractor_body_with_roof';",
          "variable.hand_tractor_body_with_wheel = query.get_equipped_item_name('main_hand') == 'tractor_body_with_wheel';",
          "variable.hand_tractor_body_with_wheels = query.get_equipped_item_name('main_hand') == 'tractor_body_with_wheels';",
          "variable.hand_tractor_body = query.get_equipped_item_name('main_hand') == 'tractor_body';",
          "variable.hand_tractor_front_wheel = query.get_equipped_item_name('main_hand') == 'tractor_front_wheel';",
          "variable.hand_tractor_front_wheels = query.get_equipped_item_name('main_hand') == 'tractor_front_wheels_on_axis';",
          "variable.hand_tractor_rear_wheel = query.get_equipped_item_name('main_hand') == 'tractor_rear_wheel';",
          "variable.hand_tractor_rear_wheels = query.get_equipped_item_name('main_hand') == 'tractor_rear_wheels_on_axis';",
          "variable.hand_tractor_roof = query.get_equipped_item_name('main_hand') == 'tractor_roof';",
          "variable.hand_tractor_steering_wheel = query.get_equipped_item_name('main_hand') == 'tractor_steering_wheel';"
        ],
        "animate": [
         { "tractor_tool": "variable.hand_tractor_tool && !variable.hand_tractor_body_with_roof && !variable.hand_tractor_body_with_wheel && !variable.hand_tractor_body_with_wheels && !variable.hand_tractor_body && !variable.hand_tractor_front_wheels && !variable.hand_tractor_rear_wheels && !variable.hand_tractor_roof && !variable.hand_tractor_steering_wheel" },
         { "tractor_body": "variable.hand_tractor_body_with_roof || variable.hand_tractor_body_with_wheel || variable.hand_tractor_body_with_wheels || variable.hand_tractor_body" },
         { "tractor_roof": "variable.hand_tractor_roof" },
         { "tractor_wheels": "variable.hand_tractor_front_wheels || variable.hand_tractor_rear_wheels" },
         { "tractor_body.first_person": "(variable.hand_tractor_body_with_roof || variable.hand_tractor_body_with_wheel || variable.hand_tractor_body_with_wheels || variable.hand_tractor_body || variable.hand_tractor_roof) && variable.is_first_person" },
         { "tractor_steering_wheel.first_person": "variable.hand_tractor_steering_wheel && variable.is_first_person" },
         { "tractor_tool.first_person": "variable.hand_tractor_tool && !variable.hand_tractor_body_with_roof && !variable.hand_tractor_body_with_wheel && !variable.hand_tractor_body_with_wheels && !variable.hand_tractor_body && !variable.hand_tractor_front_wheel && !variable.hand_tractor_front_wheels && !variable.hand_tractor_rear_wheel && !variable.hand_tractor_rear_wheels && !variable.hand_tractor_roof && !variable.hand_tractor_steering_wheel && variable.is_first_person" },
         { "tractor_wheel.first_person": "(variable.hand_tractor_front_wheel || variable.hand_tractor_rear_wheel) && variable.is_first_person" },
         { "tractor_wheels.first_person": "(variable.hand_tractor_front_wheels || variable.hand_tractor_rear_wheels) && variable.is_first_person" },
          { "in_moving_tractor": "query.mark_variant == 1" },
          { "ride_tractor": "!variable.is_first_person" },
          "root"
        ]
      },
      "animations": {
        "root": "controller.animation.player.root",
        "base_controller": "controller.animation.player.base",
        "hudplayer":  "controller.animation.player.hudplayer",
        "humanoid_base_pose": "animation.humanoid.base_pose",
        "look_at_target": "controller.animation.humanoid.look_at_target",
        "look_at_target_ui": "animation.player.look_at_target.ui",
        "look_at_target_default": "animation.humanoid.look_at_target.default",
        "look_at_target_gliding": "animation.humanoid.look_at_target.gliding",
        "look_at_target_swimming": "animation.humanoid.look_at_target.swimming",
        "look_at_target_inverted": "animation.player.look_at_target.inverted",
        "cape": "animation.player.cape",
        "move.arms": "animation.player.move.arms",
        "move.legs": "animation.player.move.legs",
        "swimming": "animation.player.swim",
        "swimming.legs": "animation.player.swim.legs",
        "in_moving_tractor": "animation.player.in_moving_tractor",
        "riding.arms": "animation.player.riding.arms",
        "riding.legs": "animation.player.riding.legs",
        "riding.tractor": "animation.player.riding.tractor",
        "ride_tractor": "controller.animation.player.ride_tractor",
        "holding": "animation.player.holding",
        "brandish_spear": "animation.humanoid.brandish_spear",
        "charging": "animation.humanoid.charging",
        "attack.positions": "animation.player.attack.positions",
        "attack.rotations": "animation.player.attack.rotations",
        "sneaking": "animation.player.sneaking",
        "bob": "animation.player.bob",
        "damage_nearby_mobs": "animation.humanoid.damage_nearby_mobs",
        "bow_and_arrow": "animation.humanoid.bow_and_arrow",
        "use_item_progress": "animation.humanoid.use_item_progress",
        "skeleton_attack": "animation.skeleton.attack",
        "sleeping": "animation.player.sleeping",
        "first_person_base_pose": "animation.player.first_person.base_pose",
        "first_person_empty_hand": "animation.player.first_person.empty_hand",
        "first_person_swap_item": "animation.player.first_person.swap_item",
        "first_person_attack_controller": "controller.animation.player.first_person_attack",
        "first_person_attack_rotation": "animation.player.first_person.attack_rotation",
        "first_person_vr_attack_rotation": "animation.player.first_person.vr_attack_rotation",
        "first_person_walk": "animation.player.first_person.walk",
        "first_person_map_controller": "controller.animation.player.first_person_map",
        "first_person_map_hold": "animation.player.first_person.map_hold",
        "first_person_map_hold_attack": "animation.player.first_person.map_hold_attack",
        "first_person_map_hold_off_hand": "animation.player.first_person.map_hold_off_hand",
        "first_person_map_hold_main_hand": "animation.player.first_person.map_hold_main_hand",
        "first_person_crossbow_equipped": "animation.player.first_person.crossbow_equipped",
        "third_person_crossbow_equipped": "animation.player.crossbow_equipped",
        "third_person_bow_equipped": "animation.player.bow_equipped",
        "crossbow_hold": "animation.player.crossbow_hold",
        "crossbow_controller": "controller.animation.player.crossbow",
        "shield_block_main_hand": "animation.player.shield_block_main_hand",
        "shield_block_off_hand": "animation.player.shield_block_off_hand",
        "blink": "controller.animation.persona.blink",
        "tractor_tool": "animation.player.hand.tractor_tool",
        "tractor_body": "animation.player.hand.tractor_body",
        "tractor_roof": "animation.player.hand.tractor_roof",
        "tractor_wheels": "animation.player.hand.tractor_wheels",
        "tractor_body.first_person": "animation.player.tractor_body.first_person",
        "tractor_steering_wheel.first_person": "animation.player.tractor_steering_wheel.first_person",
        "tractor_tool.first_person": "animation.player.tractor_tool.first_person",
        "tractor_wheel.first_person": "animation.player.tractor_wheel.first_person",
        "tractor_wheels.first_person": "animation.player.tractor_wheels.first_person"
      },
      "render_controllers": [
        { "controller.render.player.first_person": "variable.is_first_person" },
        { "controller.render.player.third_person": "!variable.is_first_person && !variable.map_face_icon" },
        { "controller.render.player.map": "variable.map_face_icon" },
        { "controller.render.player.harvester": "variable.hand_harvester" },
        { "controller.render.player.mower": "variable.hand_mower" },
        { "controller.render.player.plow": "variable.hand_plow" },
        { "controller.render.player.sprinkler": "variable.hand_sprinkler" },
        { "controller.render.player.tractor_body_with_roof": "variable.hand_tractor_body_with_roof" },

        { "controller.render.player.tractor_body_with_roof_h": "variable.hand_tractor_body_with_roof" },
        { "controller.render.player.tractor_body_with_wheel": "variable.hand_tractor_body_with_wheel" },
        { "controller.render.player.tractor_body_with_wheel_h": "variable.hand_tractor_body_with_wheel" },
        { "controller.render.player.tractor_body_with_wheels": "variable.hand_tractor_body_with_wheels" },
        { "controller.render.player.tractor_body_with_wheels_h": "variable.hand_tractor_body_with_wheels" },
        { "controller.render.player.tractor_body": "variable.hand_tractor_body" },
        { "controller.render.player.tractor_body_h": "variable.hand_tractor_body" },
        { "controller.render.player.tractor_front_wheel": "variable.hand_tractor_front_wheel" },
        { "controller.render.player.tractor_front_wheels": "variable.hand_tractor_front_wheels" },
        { "controller.render.player.tractor_rear_wheel": "variable.hand_tractor_rear_wheel" },
        { "controller.render.player.tractor_rear_wheels": "variable.hand_tractor_rear_wheels" },
        { "controller.render.player.tractor_roof": "variable.hand_tractor_roof" },
        { "controller.render.player.tractor_roof_h": "variable.hand_tractor_roof" },
        { "controller.render.player.tractor_steering_wheel": "variable.hand_tractor_steering_wheel" }
      ],
      "enable_attachables": true
    }
  }
}
