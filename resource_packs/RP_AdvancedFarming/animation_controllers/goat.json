{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.goat.move": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [
            {
              "grazing": "query.is_grazing"
            }
          ]
        },
        "grazing": {
          "animations": [ "grazing" ],
          "transitions": [
            {
              "default": "query.all_animations_finished"
            }
          ]
        }
      }
    }
  }
}